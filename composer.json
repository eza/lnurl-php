{
  "name": "ezadr/lnurl-php",
  "description": "PHP implementation of LNURL Spec, including LNURL Auth",
  "type": "library",
  "homepage": "https://github.com/eza/lnurl-php",
  "license": "DBAD",
  "authors": [
    {
      "name": "Enzo Zadrima",
      "role": "Author"
    }
  ],
  "autoload": {
